{
  "manifest_version": 3,
  "name": "__MSG_appName__",
  "description": "__MSG_appDescription__",
  "version": "1.0.0",
  "default_locale": "en",
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "host_permissions": ["http://192.168.12.207/*", "http://192.168.31.106/*"],
  "permissions": ["contextMenus"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["http://192.168.12.207/*", "http://192.168.31.106/*"],
      "js": ["content_scripts.js"],
      "run_at": "document_end"
    }
  ]
}

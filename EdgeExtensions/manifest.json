{
  "name": "CustomExtensionsUNCPath",
  "description": "ブラウザからUNCパスを開く拡張機能",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "logo_red.png",
    "128": "logo_red.png"
  },
  "permissions": ["contextMenus"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["http://192.168.12.207/*", "http://192.168.31.106/*"],
      "js": ["content_scripts.js"],
      "run_at": "document_end"
    }
  ]
}
